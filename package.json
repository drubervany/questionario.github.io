{
  "name": "questionario",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "e2e": "tsc e2e/*.ts && concurrently \"npm start\" \"protractor protractor.config.js\" --kill-others --success first",
    "e2e-live": "tsc e2e/*.ts && protractor protractor.config.js",
    "lint": "tslint ./src/app/**/*.ts -t verbose",
    "pree2e": "webdriver-manager update",
    "postinstall": "typings install",
    "start": "webpack-dev-server --inline --progress --port 8080",
    "test": "karma start",
    "test-once": "karma start --single-run",
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "crud": "node ./scaffolding/crud-script.js",
    "start-prod": "npm run build && node index.js",
    "heroku-postbuild": "npm run build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.2.4",
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/forms": "^4.2.4",
    "@angular/http": "^4.2.4",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/router": "^4.2.4",
    "bootstrap": "^4.0.0-beta.2",
    "core-js": "^2.4.1",
    "express": "^4.16.2",
    "ngx-bootstrap": "^1.9.3",
    "rxjs": "^5.4.2",
    "zone.js": "^0.8.14",
	"@angular/cli": "1.4.9",
    "@angular/compiler-cli": "^4.2.4",
    "@angular/language-service": "^4.2.4",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.2.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.3.3"
  }
}
